   "source": [
    "def tag(func):\n",
    "    def wrapper(text):\n",
    "        value = func(text)\n",
    "        return '<p>'+value+'</p>'\n",
    "    return wrapper\n",
    "\n",
    "@tag\n",
    "def my_upper(text):\n",
    "    value = text.upper()\n",
    "    return value\n",
    "print(my_upper('hello'))"
   ]
